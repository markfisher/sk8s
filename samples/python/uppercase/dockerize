#!/bin/bash

set -ex

cd "$(dirname "$0")"
if [ -e proto ]; then
  rm -fr proto/
fi
mkdir proto

GIT_PATH=https://raw.githubusercontent.com/projectriff/function-proto/master

wget $GIT_PATH/fntypes.proto -P proto
wget $GIT_PATH/function.proto -P proto

docker build . --rm -t projectriff/grpc-py:v0001
